{
  "name": "nodejs-eslint-prettier",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write .",
    "stylelint": "npx stylelint '**/*.scss'",
    "stylelint:fix": "npx stylelint '**/*.scss' --fix",
    "linters": "npm run format; npm run lint; npm run stylelint",
    "linters:fix": "npm run format; npm run lint:fix; npm run stylelint:fix"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.0.0",
    "prettier": "^3.7.4",
    "sass": "^1.97.1",
    "stylelint": "^16.26.1",
    "stylelint-config-standard-scss": "^16.0.0"
  }
}
